{
  "expo": {
    "name": "TRYNDRAW",
    "slug": "TRYNDRAW",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "backgroundColor": "#ffffff",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "backgroundColor": "#ffffff"
      
    },
    "android": {
      "backgroundColor": "#FFFFFF",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png"
      }
    },
    "web": {
      "favicon": "./assets/favicon.ico"
    }
  }
}
